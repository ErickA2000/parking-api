{
  "name": "express-typescript-template",
  "version": "1.0.0",
  "description": "Template for application in nodejs with express and typescript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon dist/server.js",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc -p tsconfig.json && (concurrently \"tsc -p tsconfig.json -w\" \"tsc-alias -p tsconfig.json -w\")",
    "lint": "eslint --ignore-path .gitignore . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ErickA2000/express-typescript-template.git"
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "docker"
  ],
  "author": "Erick Arevalo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErickA2000/express-typescript-template/issues"
  },
  "homepage": "https://github.com/ErickA2000/express-typescript-template#readme",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-query-parser": "1.3.3",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.2",
    "prisma": "^5.14.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
